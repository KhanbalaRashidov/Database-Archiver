{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "JobConfiguration": {
    "Name": "Example Job Name",
    "Cron": "0/20 * * ? * *"
  },

  "ArchiverConfiguration": {
    "Source": {
      "Provider": "MSSQL",
      "Host": "localhost",
      "TransferQuantity": 10,
      "Settings": {
        "ConnectionString": "Server=localhost;Database=AdventureWorks2022;Trusted_Connection=True;TrustServerCertificate=True",
        "Schema": "HumanResources",
        "Table": "Department",
        "IdColumn": "DepartmentID"
      }
    },
    "Target": {
      "Provider": "MSSQL",
      "Host": "localhost",
      "Settings": {
        "ConnectionString": "Server=localhost;Database=AdventureWorks2022;Trusted_Connection=True;TrustServerCertificate=True",
        "Schema": "HumanResources",
        "Table": "DepartmentCopy",
        "IdColumn": "DepartmentID"
      }
    }
  }
}
